<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="ecommerceproject.entity.status" table="status">
    <id column="status_id" name="status_id" type="int">
      <generator class="assigned"/>
    </id>
    <property column="status_name" name="status_name" type="string"/>
  </class>
</hibernate-mapping>
