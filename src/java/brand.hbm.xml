<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="ecommerceproject.entity.brand" table="brand">
    <id column="brand_id" name="brand_id" type="int">
      <generator class="assigned"/>
    </id>
    <property column="brand_name" name="brand_name" type="string"/>
  </class>
</hibernate-mapping>
