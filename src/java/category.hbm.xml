<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="ecommerceproject.entity.category" table="category">
    <id column="category_id" name="category_id" type="int">
      <generator class="assigned"/>
    </id>
    <property column="category_name" name="category_name" type="string"/>
  </class>
</hibernate-mapping>
